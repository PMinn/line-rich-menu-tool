<!DOCTYPE html>
<html lang="en">

<head>
  <%-partial('head')%>
  <link rel="stylesheet" href="/css/index.css">
</head>

<body>
  <div class="row row-cols-1 row-cols-md-5 g-4">
    <% for(var i=0; i<richmenus.length; i++) { %>
    <div class="col">
      <div class="card">
        <img src="<%= images[i] %>" class="card-img-top" alt="This richmenus No image">
        <div class="card-body">
          <h5 class="card-title"><%= richmenus[i].name %></h5>
          <button type="button" class="btn btn-outline-success">Set default</button>
          <button type="button" class="btn btn-outline-secondary">More details</button>
          <button type="button" class="btn btn-outline-danger">Delete</button>
        </div>
      </div>

    </div>
    <% } %>
  </div>
</body>

</html>